{
    "name": "indigophp/stack-guardian",
    "description": "Guardian middleware for StackPHP",
    "license": "MIT",
    "keywords": ["authentication", "guardian", "stack"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "indigophp/guardian": "dev-master",
        "symfony/http-foundation": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Guardian\\Stack\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
